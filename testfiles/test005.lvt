\documentclass{article} 
\usepackage[T1]{fontenc}
\input{regression-test} 
\usepackage[debug]{jsonparse} 

\begin{document}

\START

\SEPARATOR

\JSONParse[values only=false]{\theJSONdataA}{ { "a" : { "b" : [ "c" , "d" ] } } }

\JSONParse[values only=false]{\theJSONdataB}{ { "d" : "\x{theJSONdataA}{a.b[0]}" } }

\ExplSyntaxOn

\TEST { parsing , ~ nested } {

  \str_if_eq:enTF { \JSONParseExpandableValue{\theJSONdataB}{d} } { c } {
    \TRUE
  } {
    \FALSE
  }

}

\ExplSyntaxOff


\JSONParseSet{
  zero-based=false
}

\JSONParse[values only=false]{\theJSONdataC}{ { "a" : [ "b" , "c" , "d" ] } }

\ExplSyntaxOn

\TEST { parsing , ~ not ~ zero-based } {
  \str_if_eq:enTF { \JSONParseExpandableValue{\theJSONdataC}{a[2]} } { c } {
    \TRUE
  } {
    \FALSE
  }
}

\ExplSyntaxOff

\END

\end{document}
