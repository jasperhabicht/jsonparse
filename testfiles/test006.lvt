\documentclass{article} 
\usepackage[T1]{fontenc}
\input{regression-test} 
\usepackage{jsonparse} 

\begin{document}

\START

\JSONParseSet{
  replace={
    backspace={<<DL>>},
    formfeed={<<FF>>},
    linefeed={<<LF>>},
    carriage return={<<CR>>},
    horizontal tab={<<HT>>}
  }
}

\JSONParse{\myJSONdataX}{ { "a" : "ab\bcd\fef" , "b" : "ab\ncd\ref" , "c" : "ab\tcd" } }

\JSONParseValue[store in={\myJSONvalueA}]{\myJSONdataX}{a}
\JSONParseValue[store in={\myJSONvalueB}]{\myJSONdataX}{b}
\JSONParseValue[store in={\myJSONvalueC}]{\myJSONdataX}{c}

\ExplSyntaxOn

\TEST { parsing , ~ custom ~ keyword ~ replacements } {
  \str_if_eq:enTF { \myJSONvalueA } { ab<<DL>>cd<<FF>>ef } {
    \TRUE
  } {
    \FALSE
  }

  \str_if_eq:enTF { \myJSONvalueB } { ab<<LF>>cd<<CR>>ef } {
    \TRUE
  } {
    \FALSE
  }

  \str_if_eq:enTF { \myJSONvalueC } { ab<<HT>>cd } {
    \TRUE
  } {
    \FALSE
  }
}

\ExplSyntaxOff

\END

\end{document}
